{
	"name": "ramprasadm1986/yii2-openapi",
	"description": "Generate full REST API application from OpenAPI 3 specification.",
	"keywords": ["yii2", "rest", "openapi"],
	"homepage": "https://github.com/ramprasadm1986/yii2-openapi",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Ramprasad",
			"email": "ramprasad@krititech.in",
			
		
		}
	],
	"support": {
		"issues": "https://github.com/ramprasadm1986/yii2-openapi/issues",
		"source": "https://github.com/ramprasadm1986/yii2-openapi"
	},
	"require": {
		"php": ">=7.1.0",
		"cebe/php-openapi": "^1.1",
		"yiisoft/yii2": "~2.0.15",
		"yiisoft/yii2-gii": "~2.0.0 | ~2.1.0",
		"fakerphp/faker": "*"
	},
	"require-dev": {
		"cebe/indent": "*",
		"friendsofphp/php-cs-fixer": "~2.13.1",
		"phpunit/phpunit": "^6.5",
		"yiisoft/yii2-gii": ">=2.1.0"
	},
	"autoload": {
		"psr-4": {
			"ramprasadm1986\\yii2openapi\\": "src/"
		}
	},
	"config": {
		"platform": {
			"php": "7.1.3"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	]
}
